@model IEnumerable<Emp>

<h2>Employee List</h2>

<p>@Html.ActionLink("Add New Employee", "AddNewEmp", "Emp")</p>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Department</th>
            <th>Salary</th>
            <th>Email</th>
            <th>Phone</th>
            <th>  </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.Name</td>
                <td>@emp.Department</td>
                <td>@emp.Salary</td>
                <td>@emp.Email</td>
                <td>@emp.Phone</td>
                <td><a href="update/@emp.Id">Update</a></td>

            </tr>
        }

    </tbody>
</table>
